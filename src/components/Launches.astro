---
import { getLatestsLaunches} from '../fetchs/spacex'
import CardLaunch from './CardLaunch.astro'

const launches=await getLatestsLaunches()

---
<div class="grid gap-8 sm:grid-cols-2 lg:grid-cols-3">
    {
        launches.map(launch=>{
            return <CardLaunch
                obj={launch}
            />
        })
    }
</div>